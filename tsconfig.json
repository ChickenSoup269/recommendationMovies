{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "rootDir": "./backend",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node"
  },
  "include": ["backend/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}